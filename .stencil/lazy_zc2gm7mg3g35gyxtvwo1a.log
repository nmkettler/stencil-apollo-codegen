        return (h("div", null, h("button", { onClick: () => this.changeStatus() }, "Change Agent Status")));
    }
    render() {
        return (h("apollo-provider", { client: client }, this.renderTestButton(), h("apollo-all-posts", { renderer: ({ data, loading }) => {
